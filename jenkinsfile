pipeline {
   agent any    
   stages {
       stage('Build') {
           steps {
               sh label: '', script: '''#! /bin/bash
               sudo docker build .'''
           }
       }
       stage('ImageID') {
           steps {
               sh label: '', script: '''#! /bin/bash
               sudo docker images'''
           }
       }
       stage('Tagging') {
           steps {
               sh label: '', script: '''#! /bin/bash
               docker build -t ashurana31/firstimage:AutomatePush'''
           }
}
stage('Pushing') {
           steps {
               sh label: '', script: '''#! /bin/bash
               docker push ashurana31/AutomatePush'''
           }
}
          
     }
}
