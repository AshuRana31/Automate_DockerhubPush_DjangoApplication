pipeline {
   agent any    
   aaastages {
       stage('Build') {
           steps {
               sh label: '', script: '''#! /bin/bash
               sudo docker build .'''
           }
       }
       stage('ImageID') {
           steps {
               sh label: '', script: '''#! /bin/bash
               sudo docker images'''
           }
       }
          
     }
}
