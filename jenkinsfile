
node
{
def app


pipeline {
    agent any

    stages {
        stage('Build') {
            app = docker.build("ashurana31/firstimage")
        }
        stage('Pushing') {
            
                docker.withRegistry('','Dockerhub_credentials')
                {
                  app.push('$(env.BUILD_NUMBER)')
                  app.push("latest")
                }
            
        }
        
    }
}
}
